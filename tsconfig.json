{
    "compilerOptions": {
      "baseUrl": ".",
      "target": "ES2015",
      "module": "ES2015",
      "strict": false,
      "removeComments": false,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "sourceMap": true,
      "paths": {
        "@pages/*":["pageActions/*"],
        "@objects/*":["pageObjects/*"],
        "@extensions/*":["extensions/*"],
        "@navigations/*":["navigations/*"]
      }
    }
  }